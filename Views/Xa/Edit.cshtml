@model Web_Pro.Entities.Xa

<div class="row">
    <div class="col-xl-12">
        <div class="card">
            <div class="card-header align-items-center d-flex">
                <form asp-controller="Xa" asp-action="Edited" method="post">
                    <input type="hidden" value="@Model.MaX" asp-for="@Model.MaX" />
                    <div class="mb-3">
                        <label for="exampleInputPassword1" class="form-label">Tên Xã</label>
                        <input type="text" class="form-control" asp-for="@Model.Ten" value="@Model.Ten" id="exampleInputPassword1">
                    </div>
                    <div class="mb-3">
                        <label for="exampleInputPassword1" class="form-label">Cấp</label>
                        <input type="text" class="form-control" asp-for="@Model.Cap" value="@Model.Cap" id="exampleInputPassword1">
                    </div>
                    <div class="mb-3">
                        <label for="exampleInputPassword1" class="form-label">Huyện mẹ</label>
                        <input type="text" class="form-control" asp-for="@Model.MaH" value="@Model.MaH" id="exampleInputPassword1">
                    </div>

                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>
